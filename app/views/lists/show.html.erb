<div class="container col-lg-8 mb-5">
  <h1><%= @list.name %></h1>
  <% @list.movies.each do |movie| %>
  <%= movie.title %>
  <% end  %>
  <%= link_to 'Back to lists', lists_path, class: "btn btn-outline-secondary" %>
  <%= link_to 'Add a movie', new_list_bookmark_path(@list), class: "btn btn-outline-primary" %>
</div>
